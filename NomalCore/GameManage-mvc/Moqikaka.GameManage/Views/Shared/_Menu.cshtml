@using Moqikaka.GameManage
@{
    Layout = null;
    var currentNav = MenuProvider.GetMenuFromCurrentLoginUser();
}

@Html.CreateMenu(currentNav, (x, y) =>
{
    if (!String.IsNullOrEmpty(x) && !String.IsNullOrEmpty(y))
    {
        return System.Web.HttpUtility.UrlDecode(Url.Action(x, y));
    }
    else
    {
        return "javascript:;";
    }
}, false)